<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm ="clr-namespace:IoSilownia.ViewModel"
                    xmlns:view ="clr-namespace:IoSilownia.View">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home/>

    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CustomerVM}">
        <view:Customer/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:FormVM}">
        <view:Form/>
    </DataTemplate>


</ResourceDictionary>